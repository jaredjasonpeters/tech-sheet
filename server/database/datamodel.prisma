type User {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  email: String!@unique
  password: String!
  name: String!
  companies: [Company!]
  techsheets: [Techsheet!]!
}

type Techsheet {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  title: String!
  published: Boolean! @default(value: "false")
  author: User
}

enum Company {
  DLFPICKSEED
  SEEDRESEARCHOFOREGON
}